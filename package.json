{
  "name": "testapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./build/app.js",
    "build": "babel ./src/ --out-dir build && npm run copy:views && npm run copy:public",
    "copy:views": "copyfiles -u 2 ./src/views/* ./build/views",
    "copy:public": "copyfiles -u 2 ./src/public/**/* ./build/public",
    "dev": "babel-watch ./src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "~1.17.1",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "cuid": "^1.3.8",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "express": "~4.15.2",
    "express-session": "^1.15.5",
    "jade": "~1.11.0",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "mongoose": "^4.11.7",
    "morgan": "~1.8.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.1.0",
    "path": "^0.12.7",
    "q": "^1.5.0",
    "sendgrid": "^5.2.2",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-watch": "^2.0.7",
    "child_process": "^1.0.2",
    "concurrently": "^3.5.0",
    "copyfiles": "^1.2.0",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "nodemon-webpack-plugin": "^0.1.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  }
}
